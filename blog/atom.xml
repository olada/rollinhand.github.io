<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Kivio - Blogging for DevOps</title>
	<link href="http://rollinhand.github.io/blog/atom.xml" rel="self"/>
	<link href="http://rollinhand.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://rollinhand.github.io/blog</id>
	<author>
		<name>Bj√∂rn Berg</name>
		<email>rollin.hand@gmx.de</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://rollinhand.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://rollinhand.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>
